package guay.philippe.capstone.player.repository;

import guay.philippe.capstone.completedquiz.repository.CompletedQuiz;

import java.util.Date;

import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

public class Player {

	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	private long id;
	
	private String username;
	private String password_hash;
	private Date creation_time;
	private int score;
	private CompletedQuiz[] completed_quizzes;
	
	public Player() {
	}
	
	public Player(String username, String password_hash) {
		
	}
}

