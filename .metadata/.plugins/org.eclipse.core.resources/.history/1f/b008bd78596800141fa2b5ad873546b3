package guay.philippe.capstone;

import java.util.ArrayList;

public class Quiz implements Parcelable{
	
	private String name;
	private int rating;
	private String justification;
	private int unrelatedMovie;
	private ArrayList<String> movieSet;
	
	public Quiz(String name, int rating, String justification, int unrelatedMovie, ArrayList<String> movieSet){
		this.name = name;
		this.rating = rating;
		this.justification = justification;
		this.unrelatedMovie = unrelatedMovie;
		this.movieSet = movieSet;
	}
	
	public int getRating() {
		return rating;
	}
	
	public String getName() {
		return name;
	}
	
	public int getUnrelatedMovie() {
		return unrelatedMovie;
	}
}
